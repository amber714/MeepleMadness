<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.AddGameController">
   <children>
      <StackPane prefHeight="800.0" prefWidth="800.0" styleClass="background" stylesheets="@../application.css">
         <children>
            <Line disable="true" endX="160.0" fill="BLUE" startX="-160.0" stroke="BLUE" strokeWidth="5.0" translateY="-175.0" />
            <Label text="Add Game" textFill="WHITE" translateY="-200.0">
               <font>
                  <Font size="59.0" />
               </font>
            </Label>
            <Label text="Game title:  " textFill="WHITE" translateX="-100.0" translateY="-100.0">
               <font>
                  <Font size="27.0" />
               </font>
            </Label>
            <TextField fx:id="gameTitle" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="39.0" prefWidth="228.0" translateX="100.0" translateY="-100.0">
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor>
            </TextField>
            <Label layoutX="344.0" layoutY="294.0" text="Console: " textFill="WHITE" translateX="-100.0" translateY="-30.0">
               <font>
                  <Font size="27.0" />
               </font>
            </Label>
            <TextField fx:id="console" layoutX="296.0" layoutY="291.0" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="39.0" prefWidth="228.0" translateX="100.0" translateY="-30.0">
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor>
            </TextField>
            <CheckBox fx:id="completed" mnemonicParsing="false" text="Completed" textFill="WHITE" translateX="-80.0" translateY="100.0">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </CheckBox>
            <CheckBox fx:id="inProgress" layoutX="368.0" layoutY="301.0" mnemonicParsing="false" text="In Progress" textFill="WHITE" translateX="80.0" translateY="100.0">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </CheckBox>
            <Button fx:id="save" mnemonicParsing="false" onAction="#handle" prefHeight="60.0" prefWidth="86.0" text="Save" translateX="280.0" translateY="200.0">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button fx:id="back" layoutX="367.0" layoutY="380.0" mnemonicParsing="false" onAction="#handle" prefHeight="50.0" prefWidth="64.0" text="Back" translateX="-300.0" translateY="210.0">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
         </children>
      </StackPane>
   </children>
</AnchorPane>
